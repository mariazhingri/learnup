{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "css-prefix-order": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "button-type": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 125",
    "not firefox <= 125"
  ]
}